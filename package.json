  "version": "0.1.0-alpha.0",
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-theme",
    "css-in-js"
  ],
  "author": {
    "name": "Hyeseong Kim",
    "email": "cometkim.kr@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometkim/gatsby-theme-stitches.git"
  },
  "scripts": {
    "dts": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "babel . --extensions .ts,.tsx --ignore .yarn --out-dir .",
