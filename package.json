  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@stitches/react": "^0.1.4",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@yarnpkg/pnpify": "^2.2.1",
    "babel-preset-gatsby-package": "^0.5.2",
